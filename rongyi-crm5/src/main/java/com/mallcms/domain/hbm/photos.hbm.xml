<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="true" package="com.mallcms.domain">
 <class dynamic-insert="false" dynamic-update="true"
  name="com.mallcms.domain.PhotosPojo" table="photos">
  <id column="id" name="id" type="java.lang.Integer">
   <generator class="native"/>
  </id>
  <property name="createdAt" column="created_at" type="java.util.Date" />   
  <property name="updatedAt" column="updated_at" type="java.util.Date" />  
  <property name="file" column="file" type="java.lang.String" /> 
  <property name="ownerId" column="owner_id" type="java.lang.Integer" /> 
  <property name="ownerType" column="owner_type" type="java.lang.String" /> 
  <property name="position" column="position" type="java.lang.Integer" /> 
  <property name="ownerMongoId" column="owner_mongo_id" type="java.lang.String" /> 
  <property name="fileUrl" column="file_url" type="java.lang.String" /> 
  <property name="mongoId" column="mongo_id" type="java.lang.String" /> 
  <property name="mallId" column="mall_id" type="java.lang.String" /> 
  <property name="cmsPhotoId" column="cms_photo_id" type="java.lang.Integer" /> 
 </class>
</hibernate-mapping>
