<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="true" package="com.mallcms.domain">
 <class dynamic-insert="false" dynamic-update="true"
  name="com.mallcms.domain.CmsTask" table="cms_task">
  <id column="id" name="id" type="java.lang.Integer">
   <generator class="native"/>
  </id>
  <property name="gmtCreate" column="gmtCreate" type="java.util.Date" />   
  <property name="gmtModified" column="gmtModified" type="java.util.Date" />  
  <property name="taskStatus" column="task_status" type="java.lang.Integer" /> 
  <property name="type" column="type" type="java.lang.Integer" /> 
  <property name="brandUrl" column="brand_url" type="java.lang.String" /> 
  <property name="mallName" column="mall_name" type="java.lang.String" /> 
  <property name="gmtJobCreate" column="gmtJobCreate" type="java.util.Date" /> 
  <property name="actionType" column="action_type" type="java.lang.Integer" /> 
   <property name="reviewStatus" column="reviewStatus" type="java.lang.Integer" /> 
   <property name="showInfo" column="show_info" type="java.lang.String" /> 
   <property name="mallId" column="mall_id" type="java.lang.String" /> 
   <property name="cmsOuterId" column="cms_outerId" type="java.lang.String" /> 
   <property name="reviewInfo" column="review_info" type="java.lang.String" /> 
 </class>
</hibernate-mapping>
