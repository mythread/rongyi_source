<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-lazy="true" package="com.gcrm.domain">
 <class dynamic-insert="true" dynamic-update="true"
  name="com.gcrm.domain.ReminderOption" table="reminderoption">
  <id column="id" name="id" type="java.lang.Integer">
   <generator class="native"/>
  </id>
  <property column="value" generated="never" insert="true" lazy="false"
   length="50" name="value" not-null="true" type="java.lang.String" update="true"/>
  <property column="label_en_US" generated="never" insert="true" lazy="false"
   length="50" name="label_en_US" type="java.lang.String" update="true"/>   
  <property column="label_zh_CN" generated="never" insert="true" lazy="false"
   length="50" name="label_zh_CN" type="java.lang.String" update="true"/>    
  <property column="sequence" generated="never" insert="true"
   lazy="false" name="sequence" type="int" update="true"/>
 </class>
</hibernate-mapping>
